      </style:style>
      <text:list-style style:name="L1">
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="1"
                                       text:start-value="2"
                                       text:style-name="Numbering Symbols">
            <style:properties text:min-label-width="0.499cm"/>
