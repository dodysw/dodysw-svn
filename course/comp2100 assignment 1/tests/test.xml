                           style:auto-text-indent="false"
                           fo:font-weight="normal"
                           style:text-background-color="transparent"
                           fo:text-transform="none"
                           style:font-name="Nimbus Roman No9 L"
                           fo:font-variant="normal"
                           fo:text-indent="0cm"/>
      </style:style>
      <style:style style:family="paragraph"
                   style:parent-style-name="Footnote"
                   style:name="P11">
         <style:properties fo:text-indent="0cm"
                           style:auto-text-indent="false"
                           fo:margin-left="0cm"
                           fo:margin-right="0cm"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T1">
         <style:properties style:font-weight-asian="bold"
                           style:font-weight-complex="bold"
                           fo:font-weight="bold"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T2">
         <style:properties style:font-weight-asian="normal"
                           style:font-style-asian="normal"
                           style:font-weight-complex="normal"
                           fo:font-style="normal"
                           fo:font-weight="normal"
                           style:font-style-complex="normal"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T3">
         <style:properties style:font-weight-asian="normal"
                           style:font-style-asian="italic"
                           style:font-weight-complex="normal"
                           fo:font-style="italic"
                           fo:font-weight="normal"
                           style:font-style-complex="italic"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T4">
         <style:properties style:font-weight-asian="bold"
                           style:font-style-asian="normal"
                           style:font-weight-complex="bold"
                           fo:font-style="normal"
                           fo:font-weight="bold"
                           style:font-style-complex="normal"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T5">
         <style:properties style:font-style-asian="italic"
                           fo:font-style="italic"
                           style:font-style-complex="italic"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T6">
         <style:properties style:font-style-asian="normal"
                           fo:font-style="normal"
                           style:font-style-complex="normal"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T7">
         <style:properties style:font-style-asian="normal"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T8">
         <style:properties style:font-style-asian="normal"
                           style:text-position="super 58%"/>
      </style:style>
      <style:style style:family="text"
                   style:name="T9">
         <style:properties fo:font-variant="small-caps"/>
      </style:style>
      <text:list-style style:name="L1">
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="1"
                                       text:style-name="Numbering Symbols">
            <style:properties text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="2"
                                       text:style-name="Numbering Symbols">
            <style:properties text:space-before="0.501cm"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="3"
                                       text:style-name="Numbering Symbols">
            <style:properties text:space-before="1cm"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="4"
