         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="5"
                                       text:style-name="Numbering Symbols">
            <style:properties text:space-before="2cm"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="6"
                                       text:style-name="Numbering Symbols">
            <style:properties text:space-before="2.501cm"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="7"
                                       text:style-name="Numbering Symbols">
            <style:properties text:space-before="3.001cm"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="8"
                                       text:style-name="Numbering Symbols">
            <style:properties text:space-before="3.502cm"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="9"
                                       text:style-name="Numbering Symbols">
            <style:properties text:space-before="4.001cm"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
         <text:list-level-style-number style:num-format="1"
                                       style:num-suffix="."
                                       text:level="10"
                                       text:style-name="Numbering Symbols">
            <style:properties text:space-before="4.502cm"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-number>
      </text:list-style>
      <text:list-style style:name="L2">
         <text:list-level-style-bullet style:num-suffix="."
                                       text:bullet-char="•"
                                       text:level="1"
                                       text:style-name="Bullet Symbols">
            <style:properties style:font-name="StarSymbol"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-bullet>
         <text:list-level-style-bullet style:num-suffix="."
                                       text:bullet-char="•"
                                       text:level="2"
                                       text:style-name="Bullet Symbols">
            <style:properties text:space-before="0.501cm"
                              style:font-name="StarSymbol"
                              text:min-label-width="0.499cm"/>
         </text:list-level-style-bullet>
         <text:list-level-style-bullet style:num-suffix="."
                                       text:bullet-char="•"
                                       text:level="3"
                                       text:style-name="Bullet Symbols">
            <style:properties text:space-before="1cm"
                              style:font-name="StarSymbol"
                              text:min-label-width="0.499cm"/>
